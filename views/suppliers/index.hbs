<div class="mui-container">
  <h1>All Suppliers</h1>
  <ul class='collection'>
    {{#each suppliers as |s|}}
    <li>
      <a class='mui-panel' href="/suppliers/{{s.Id}}">
        <span class='mui-badge mui--pull-right'>{{s.Id}}</span>
        <h3 class='mui-panel__title'>{{s.CompanyName}}</h3>
        <p>
          <strong>Contact Name:</strong> {{s.ContactName}}
        </p>
      </a>
    </li>
    {{else}}
    <li>No suppliers</li>
    {{/each}}
  </ul>
</div>